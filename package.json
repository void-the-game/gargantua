{
  "name": "void-api",
  "version": "1.0.0",
  "description": "Void backend",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc && yarn sentry:sourcemaps",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org void-n5 --project void-backend ./dist && sentry-cli sourcemaps upload --org void-n5 --project void-backend ./dist"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/cli": "^2.46.0",
    "@sentry/node": "^9.28.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "moongose": "^1.0.0",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
